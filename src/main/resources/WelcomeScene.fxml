<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>

<VBox alignment="CENTER" minHeight="600.0" minWidth="800.0" spacing="30.0" style="-fx-background-color: #2c3848;" xmlns="http://javafx.com/javafx/8.0.172-ea" xmlns:fx="http://javafx.com/fxml/1" fx:controller="GUI.WelcomeSceneController">
   <children>
      <ImageView fitHeight="150.0" fitWidth="800.0" pickOnBounds="true" preserveRatio="true">
         <image>
            <Image url="@logo.gif" />
         </image>
         <VBox.margin>
            <Insets top="100.0" />
         </VBox.margin>
      </ImageView>
      <Button minWidth="0.0" mnemonicParsing="false" onAction="#pushAboutButton" style="-fx-background-color: #45b7b8;" text="About the program">
         <font>
            <Font name="System Bold" size="13.0" />
         </font>
         <VBox.margin>
            <Insets top="100.0" />
         </VBox.margin>
      </Button>
      <Button mnemonicParsing="false" onAction="#pushContinueButton" style="-fx-background-color: #45b7b8;" text="Continue">
         <font>
         <Font name="System Bold" size="13.0" />
         </font>
      </Button>
   </children>
</VBox>
